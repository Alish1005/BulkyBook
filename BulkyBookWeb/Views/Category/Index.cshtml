@model IEnumerable<Category>

@{
    ViewData["Title"] = "Index";
}
<h1>Index Category</h1>

<table class="table table-bordered table-striped table-hover" style="width:100%">
    <thead>
        <tr>
            <th>Category name</th>
            <th>Display Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.DisplayOrder</td>
            </tr>
        }
    </tbody>
</table>